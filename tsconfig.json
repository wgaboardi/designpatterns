{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "esModuleInterop": false,
    "skipLibCheck": true, // acelerar compilacao
    "forceConsistentCasingInFileNames": true //nao permitir referencias inconsistentes
  },
  "include": [
    "./src"
  ],
  "exclude": []
}